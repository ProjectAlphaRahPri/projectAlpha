\par \pard\plain \s0\ql\nowidctlpar\hyphpar0\ltrpar\cf1\kerning1\dbch\af7\langfe1081\dbch\af7\afs24\alang1081\loch\f3\fs24\lang16393\sl276\slmult1\ql\nowidctlpar\hyphpar0\ltrpar\b0\rtlch \ltrch\loch\fs20\lang9\loch\f6

